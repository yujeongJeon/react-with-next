{
  "name": "leaflo-webchat-sdk",
  "version": "1.0.0",
  "description": "봇빌더 웹챗 SDK",
  "main": "index.js",
  "scripts": {
    "dev:window": "set NEXT_ENV=development&& next -p 3001",
    "dev:linux": "export NEXT_ENV=development&& next -p 3001",
    "build": "next build",
    "start:window": "set NEXT_ENV=lcloud&& npm run build&& next start -p 4001",
    "start:linux": "export NEXT_ENV=lcloud&& npm run build&& next start -p 4001",
    "storybook": "start-storybook -s ./public",
    "analyze": "set ANALYZE=true&& next build",
    "analyze:server": "set BUNDLE_ANALYZE=server&& next build",
    "analyze:browser": "set BUNDLE_ANALYZE=browser&& next build"
  },
  "repository": {
    "type": "git",
    "url": "http://s-gitlab.ldcc.co.kr/bot-builder/leaflo-webchat-sdk.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@next/bundle-analyzer": "^9.1.6",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-source-maps": "0.0.3",
    "axios": "^0.19.0",
    "css-vars-ponyfill": "^2.1.2",
    "next": "^9.2.1",
    "node-sass": "^4.13.0",
    "path": "^0.12.7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "classnames": "^2.2.6",
    "file-loader": "^5.0.2",
    "react-app-polyfill": "^1.0.5",
    "url-loader": "^3.0.0"
  }
}
